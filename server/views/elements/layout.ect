			<li><a href="/">始めに</a></li>
			<li><a href="/regist">メンバー登録</a></li>
			<li><a href="/reservation">予約</a></li>
		</ul>
	</div><!-- /.inner -->
</nav>

    <% content %>

  <!-- Angular2用テンプレート -->
  <script>
    var _template = (function() {/*
      <% content 'ag2tmp-info' %>
  */}).toString().match(/\/\*([^]*)\*\//)[1];
  </script>
  
  <!-- フッター -->
  <% include 'elements/footer' %>
